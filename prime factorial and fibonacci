

def factorial(x):
   factorial = 1
   if x<0:
        print("Enter a positive integer")
   elif x==0:
        print(factorial)
   else:
        for n in range(1, x+1):
            factorial= factorial*n

        print(factorial)


def fibonacci(x):
    if x < 0:
        print("Enter a positive integer")
 
    elif x == 0:
        return 0
 
    
    elif x == 1 or x == 2:
        return 1
 
    else:
        return fibonacci(x-1) + fibonacci(x-2)
    
    print(fibonacci(x))
 
num=int(input("Hey there. Enter any positive integer "))

for i in range(2,num):
    if num % i == 0:
        fibonacci(num)
    else: 
        factorial(num)
        


